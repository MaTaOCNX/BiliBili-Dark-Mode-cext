{
  "manifest_version": 3,
  "name": "Bilibili Dark Mode Pro",
  "version": "1.1",
  "permissions": ["storage"],
  "action": { "default_popup": "popup.html" },
  "content_scripts": [
    {
      "matches": ["*://*.bilibili.tv/*"],
      "js": ["content.js"],
      "css": ["styles.css"],
      "run_at": "document_start"
    }
  ]
}